{
  "name": "live-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve:pc": "cross-env SITE=pc vue-cli-service serve",
    "build:pc": "cross-env SITE=pc NODE_ENV=production vue-cli-service build --mode staging --dest dist/pc",
    "serve:mb": "cross-env SITE=mb vue-cli-service serve",
    "build:mb": "cross-env SITE=mb NODE_ENV=production vue-cli-service build --mode staging --dest dist/mb",
    "build:icon": "icon-font-generator svg-icons/*.svg -o src/styles/fonts"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bootstrap": "^5.0.1",
    "core-js": "^3.8.3",
    "element-plus": "^2.3.4",
    "normalize.css": "^8.0.1",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-final-modal": "^3.4.3",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "icon-font-generator": "^2.1.11",
    "lodash.merge": "^4.6.2",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "~4.5.5"
  }
}
